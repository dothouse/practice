   "source": [
    "N = int(input())\n",
    "\n",
    "groups = 1\n",
    "\n",
    "while N > groups:\n",
    "    N -= groups \n",
    "    groups += 1\n",
    "\n",
    "if groups % 2 == 0:\n",
    "    print(f'{N}/{groups - N + 1}')\n",
    "else :\n",
    "    print(f'{groups - N + 1}/{N}')"
   ]
