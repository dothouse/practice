   "outputs": [],
   "source": [
    "a = int(input())\n",
    "b = map(int, input().split())\n",
    "c = int(input())\n",
    "d = 0\n",
    "for i in b:\n",
    "    if i == c:\n",
    "        d += 1\n",
    "print(d)\n"
   ]
