    "numbers = list(map(int, input().split()))\n",
    "\n",
    "long = max(numbers)\n",
    "sum2 = sum(numbers) - max(numbers)\n",
    "\n",
    "if long >= sum2:\n",
    "    long = sum2 - 1\n",
    "print(long + sum2)"
