   "source": [
    "N = int(input())\n",
    "\n",
    "numbers = [i for i in map(int, input().split())]\n",
    "result = 0\n",
    "\n",
    "for j in numbers:\n",
    "    cnt = 0\n",
    "    for k in range(1, j):\n",
    "        if j % k == 0:\n",
    "            cnt += 1\n",
    "\n",
    "    if cnt == 1:\n",
    "        result += 1\n",
    "\n",
    "print(result)"
   ]
