   "source": [
    "total_price = int(input())\n",
    "total_num = int(input())\n",
    "total = 0\n",
    "\n",
    "for i in range(total_num):\n",
    "    a, b = map(int, input().split())\n",
    "    total += a * b\n",
    "\n",
    "if total == total_price:\n",
    "    print(\"Yes\")\n",
    "else:\n",
    "    print(\"No\")"
   ]
  }
