   "source": [
    "N = int(input())\n",
    "\n",
    "result = []\n",
    "\n",
    "for i in range(N//3 + 1):\n",
    "    for j in range(N//5 + 1):\n",
    "        if 3 * i + 5 * j == N:\n",
    "            result.append(i + j)\n",
    "\n",
    "if result == [] :\n",
    "    print(-1)\n",
    "else :\n",
    "    print(min(result))\n",
    "\n",
    "\n"
   ]
