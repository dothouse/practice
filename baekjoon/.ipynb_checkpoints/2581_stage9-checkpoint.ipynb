   "source": [
    "M = int(input())\n",
    "N = int(input())\n",
    "lst = []\n",
    "\n",
    "for i in range(M, N+1):\n",
    "    cnt = 0\n",
    "    for j in range(1, i):\n",
    "        if i % j == 0:\n",
    "            cnt += 1\n",
    "    if cnt == 1:\n",
    "        lst.append(i)\n",
    "\n",
    "if sum(lst) == 0:\n",
    "    print(-1)\n",
    "else :\n",
    "    print(sum(lst), lst[0], sep = '\\n')\n"
   ]
