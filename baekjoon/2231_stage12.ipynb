   "source": [
    "N = int(input())\n",
    "\n",
    "result = []\n",
    "for i in range(0, N + 1):\n",
    "    sum_numbers = sum(map(int, str(i)))\n",
    "    P = sum_numbers + i\n",
    "    if P == N:\n",
    "        result.append(i)\n",
    "\n",
    "if len(result) > 0 :\n",
    "    print(min(result))\n",
    "else :\n",
    "    print(0)"
   ]
