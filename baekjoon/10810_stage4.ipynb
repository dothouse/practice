    "\n",
    "lst = [0] * N\n",
    "\n",
    "for _ in range(M):\n",
    "    i, j, k = map(int, input().split())\n",
    "    for b in range(i, j+1):\n",
    "        lst[b-1] = k\n",
    "\n",
    "for i in range(N):\n",
    "    print(lst[i], end = ' ')\n",
    "    "
