    "a, b = map(int, input().split())\n",
    "\n",
    "factors = []\n",
    "\n",
    "for i in range(1, a+1):\n",
    "    if a % i == 0:\n",
    "        factors.append(i)\n",
    "\n",
    "if b <= len(factors):\n",
    "    print(factors[b-1])\n",
    "else :\n",
    "    print(0)"
